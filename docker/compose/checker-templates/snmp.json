{
  "node_address": "{{AGENT_ADDRESS}}",
  "listen_addr": ":50054",
  "security": {
    "mode": "mtls",
    "role": "checker",
    "cert_dir": "{{CERT_DIR}}",
    "server_name": "{{SERVER_NAME}}",
    "tls": {
      "cert_file": "{{CERT_DIR}}/{{CLIENT_CERT_NAME}}.pem",
      "key_file": "{{CERT_DIR}}/{{CLIENT_CERT_NAME}}-key.pem",
      "ca_file": "{{CERT_DIR}}/root.pem",
      "client_ca_file": "{{CERT_DIR}}/root.pem"
    }
  },
  "timeout": "30s",
  "logger": {
    "level": "{{LOG_LEVEL}}",
    "debug": false,
    "output": "stdout"
  },
  "targets": []
}
