{
  "listen_addr": "0.0.0.0:50110",
  "sample_interval": "250ms",
  "security": {
    "mode": "spiffe",
    "role": "checker",
    "cert_dir": "/etc/serviceradar/certs",
    "trust_domain": "PLACEHOLDER_TRUST_DOMAIN",
    "workload_socket": "unix:/run/spire/sockets/agent.sock",
    "server_spiffe_id": "spiffe://PLACEHOLDER_TRUST_DOMAIN/ns/PLACEHOLDER_NAMESPACE/sa/PLACEHOLDER_SYSMON_VM_CHECKER_SA"
  }
}
