{
    "name": "dusk",
    "type": "grpc",
    "node_address": "localhost:8080",
    "listen_addr": ":50082",
    "timeout": "5m",
    "security": {
        "server_name": "PLACEHOLDER_AGENT_SERVER_NAME",
        "mode": "spiffe",
        "role": "checker",
        "cert_dir": "/etc/serviceradar/certs",
        "trust_domain": "PLACEHOLDER_TRUST_DOMAIN",
        "server_spiffe_id": "spiffe://PLACEHOLDER_TRUST_DOMAIN/ns/PLACEHOLDER_NAMESPACE/sa/PLACEHOLDER_DUSK_CHECKER_SA",
        "workload_socket": "unix:/run/spire/sockets/agent.sock",
        "tls": {
            "cert_file": "checkers.pem",
            "key_file": "checkers-key.pem",
            "ca_file": "root.pem",
            "client_ca_file": "root.pem"
        }
    }
}
